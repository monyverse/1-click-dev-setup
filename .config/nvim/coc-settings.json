{
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " ✗ ",
  "diagnostic.virtualTextLines": 1,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagonstic.virtualTextCurrentLineOnly": true,
  "diagnostic.enableSign": true,
  "diagnostic.enableMessage": "true",
  "diagnostic.messageDelay": 0,
  "diagnostic.maxWindowHeight": 16,
  "diagnostic.format": "✗ %message",
  "diagnostic.hintSign": "",
  "diagnostic.level": "warning",
  "diagnostic.checkCurrentLine": true,
  "suggest.fixInsertedWord": false,
  "codeLens.enable": true,
  "codeLens.separator": " ",
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.snippets.enable": false,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "solidity",
    "javascript",
    "typescript",
    "python",
    "bash"
  ],
  "coc.source.snippets.enable": false,
  "coc.source.buffer.enable": false,
  "coc.source.file.enable": false,
  "rust-analyzer.inlayHints.typeHintsSeparator": "",
  "rust-analyzer.inlayHints.chainingHintsSeparator": " ",
  "rust-analyzer.updates.prompt": false,
  "rust-analyzer.diagnostics.enableExperimental": true,
  "rust-analyzer.diagnostics.disabled": [
    "macro-error"
  ],
  "rust-analyzer.rustfmt.extraArgs": [
    "+nightly"
  ],
  "rust-analyzer.lens.enable": true,
  "list.maxHeight": 10,
  "list.autoResize": false,
  "list.extendedSearchMode": false,
  "suggest.fixInsertedWord": false,
  "suggest.maxCompleteItemCount": 32
}
